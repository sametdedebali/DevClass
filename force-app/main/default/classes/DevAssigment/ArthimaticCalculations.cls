public class ArthimaticCalculations {
    public Integer Addition(Integer k, Integer l){
        Integer add = k + l;
        return add;
    }
    public Integer Subtraction(Integer x, Integer y){
        Integer sub = x - y;
        return sub;
    }
    public Integer Multiplication(Integer a, Integer b, Integer c){
        Integer multip = a * b * c;
        return multip;
    }
}
